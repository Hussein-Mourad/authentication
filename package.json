    "express-session": "^1.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.9",
    "morgan": "~1.9.1",
    "multiparty": "^4.2.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
