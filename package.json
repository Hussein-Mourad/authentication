    "@webpack-cli/generators": "^1.1.0",
    "aws-sdk": "^2.955.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
